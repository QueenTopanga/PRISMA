<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.queen.topanga.prisma">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".base.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".base.activitys.CollectionActivity"
            android:label="Collection"
            android:parentActivityName=".base.activitys.MainActivity"/>
        <activity
            android:name=".base.activitys.MissionsActivity"
            android:label="Missions"
            android:parentActivityName=".base.activitys.MainActivity"/>
        <activity
            android:name=".base.activitys.ModeActivity"
            android:label="Game Mode"
            android:parentActivityName=".base.activitys.MainActivity"/>
        <activity
            android:name=".base.activitys.SettingsActivity"
            android:label="Settings"
            android:parentActivityName=".base.activitys.MainActivity"/>
        <activity
            android:name=".base.activitys.ShopActivity"
            android:label="Shop"
            android:parentActivityName=".base.activitys.MainActivity"/>
    </application>

</manifest>